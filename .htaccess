
RewriteEngine On


<FilesMatch "^(db|config|variables|\.env|\.htaccess|inc)($|\.php)">
    Require all denied
</FilesMatch>


RewriteRule ^(config|inc)/ - [F,L]


RewriteCond %{THE_REQUEST} ^[A-Z]{3,}\s/([^.]+)\.php[\s?] [NC]
RewriteRule ^ /%1 [R=301,L]


RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.php -f
RewriteRule ^([^/]+)/?$ $1.php [L]


RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.php -f
RewriteRule ^dashboard/([^/]+)/?$ dashboard/$1.php [L]
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.php -f
RewriteRule ^organizer/([^/]+)/?$ organizer/$1.php [L]
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.php -f
RewriteRule ^admin/([^/]+)/?$ admin/$1.php [L]
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.php -f
RewriteRule ^api/([^/]+)/?$ api/$1.php [L]
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.php -f
RewriteRule ^alpha/([^/]+)/?$ alpha/$1.php [L]

ErrorDocument 404 /404.php


RewriteRule ^event/([a-zA-Z0-9\-.]+)$ event.php?slug=$1 [L,QSA]


Options -Indexes
